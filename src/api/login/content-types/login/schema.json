{
  "kind": "singleType",
  "collectionName": "logins",
  "info": {
    "singularName": "login",
    "pluralName": "logins",
    "displayName": "login",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "label": {
      "type": "string",
      "required": true
    },
    "button": {
      "type": "component",
      "repeatable": false,
      "component": "shared.button"
    },
    "input": {
      "displayName": "input",
      "type": "component",
      "repeatable": true,
      "component": "shared.input"
    },
    "bottomText": {
      "type": "component",
      "repeatable": false,
      "component": "shared.support"
    },
    "backgroundImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "logo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.logo"
    },
    "emailRegex": {
      "type": "string",
      "required": true,
      "default": "/^(([^<>()\\[\\]\\\\.,;:\\s@\"]+(\\.[^<>()\\[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/"
    },
    "emailErrorMsg": {
      "type": "string"
    },
    "passwordRegex": {
      "type": "string",
      "required": true,
      "default": "/^(?=.*\\d)(?=.*[!@#$%^&*])(?=.*[a-z])(?=.*[A-Z]).{8,}$/"
    },
    "passwordErrorMsg": {
      "type": "string"
    }
  }
}
